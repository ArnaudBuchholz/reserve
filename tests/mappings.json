{
  "mappings": [{
    "use": "express-session",
    "options" : {
      "secret": "keyboard cat",
      "resave": false,
      "saveUninitialized": true
    }
  }, {
    "custom": "./cors.js"
  }, {
    "match": "^/proxy/(https?)/(.*)",
    "url": "$1://$2",
    "unsecure-cookies": true
  }, {
    "match": "^/echo/(.*)$",
    "custom": "./echo.js"
  }, {
    "match": "^/chrome/(.*)$",
    "custom": "./chrome.js"
  }, {
    "match": "^/gpf\\.js$",
    "file": "../node_modules/gpf-js/build/gpf.js"
  }, {
    "match": "(.*)",
    "strict": true,
    "file": "./$1"
  }]
}
