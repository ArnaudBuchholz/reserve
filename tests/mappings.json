{
  "mappings": [{
    "use": "express-session",
    "options" : {
      "secret": "keyboard cat",
      "resave": false,
      "saveUninitialized": true
    }
  }, {
    "custom": "./cors.js"
  }, {
    "match": "^/url/proxy/(https?)/(.*)",
    "url": "$1://$2",
    "unsecure-cookies": true
  }, {
    "match": "^/custom/echo/(.*)$",
    "custom": "./echo.js"
  }, {
    "match": "^/status/301",
    "status": 301,
    "headers": {
      "location": "https://www.npmjs.com/package/reserve"
    }
  }, {
    "match": "^/status/redirect/(.*)",
    "status": 302,
    "headers": {
      "location": "https://www.npmjs.com/package/$1"
    }
  }, {
    "match": "^/status/404",
    "status": 404
  }, {
    "match": "^/status/508",
    "status": 508
  }, {
    "match": "^/file/(.*)",
    "strict": true,
    "file": "./$1"
  }]
}
