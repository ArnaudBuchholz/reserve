{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "mutate": [
    "*.js",
    "detect/*.js",
    "handlers/*.js",
    "mock/*.js"
  ],
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress",
    "dashboard"
  ],
  "testRunner": "mocha",
  "coverageAnalysis": "perTest",
  "ignoreStatic": true
}